{
  "name": "innos-homepage",
  "version": "1.0.0",
  "description": "React Server-side Rendering",
  "author": "Paths",
  "license": "MIT",
  "engines": {
    "node": ">= 10.15.0",
    "npm": "6.9.0"
  },
  "scripts": {
    "bundle:dev": "cross-env NODE_ENV=development webpack --config webpack.config.js",
    "start:dev": "nodemon --exec babel-node src/index.js",
    "bundle:prod": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "babel": "rimraf dist/* && babel src -d dist --copy-files",
    "build": "npm run bundle:prod && npm run babel",
    "prestart:prod": "npm run build",
    "start:prod": "node dist/index.js"
  },
  "dependencies": {
    "antd": "^3.15.2",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/plugin-transform-spread": "^7.2.2",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.2",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.11.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "ignore-styles": "^5.0.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "nodemon": "^1.18.10",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  }
}
